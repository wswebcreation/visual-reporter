{
  "name": "visual-reporter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./src/app/scripts/generateThumbnails.mjs && next dev",
    "build": "node ./src/app/scripts/generateThumbnails.mjs && NEXT_PUBLIC_BUILD_MODE=dynamic next build",
    "export": "node ./src/app/scripts/generateThumbnails.mjs && NEXT_PUBLIC_BUILD_MODE=static next build",
    "lint": "next lint",
    "serve": "next start",
    "static": "npm run export && npx serve out",
    "start": "next start"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-select": "^5.8.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "typescript": "^5"
  }
}